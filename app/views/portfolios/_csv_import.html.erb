<% form_for :upload, :url=>{:controller=>"csv", :action=>"upload", :id=> @portfolio }, :html => { :multipart => true } do |f| -%>
  <div class="span-8">
    <%= f.file_field :csv %>
  </div>
  <div class="span-3">  
    <%= f.submit "Upload", :disable_with => 'Uploading...' %>
  </div>
</br>
<% end -%>